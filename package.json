{
	"name": "techsavvy",
	"version": "1.0.0",
	"description": "A publishing platform built with React and Express",
	"repository": {
		"type": "git",
		"url": "https://github.com/sbramd/techsavvy"
	},
	"main": "server.js",
	"license": "MIT",
	"dependencies": {
		"@sendgrid/mail": "^6.3.1",
		"bcrypt": "^3.0.0",
		"body-parser": "^1.18.2",
		"cloudinary": "^1.11.0",
		"cors": "^2.8.4",
		"dotenv": "^5.0.1",
		"ejs": "^2.6.1",
		"express": "^4.16.3",
		"helmet": "^3.12.0",
		"jsonwebtoken": "^8.2.1",
		"mongoose": "^5.0.17",
		"morgan": "^1.9.0",
		"multer": "^1.3.0"
	},
	"engines": {
		"npm": "6.0.0",
		"node": "8.5.0"
	},
	"scripts": {
		"watch-js": "cd client && npm start",
		"watch-css": "cd client && npm run watch-css",
		"watch-node": "nodemon server.js",
		"dev": "concurrently --kill-others-on-fail \"npm:watch-*\"",
		"start": "node server.js",
		"_heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
	},
	"devDependencies": {
		"concurrently": "^4.0.1",
		"nodemon": "^1.17.4"
	}
}
