{
	"env": {
		"es6": true,
		"node": true,
		"browser": true
	},
	"extends": ["eslint:recommended"],
	"parserOptions": {
		"ecmaVersion": 8,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
			"globalReturn": true,
			"experimentalObjectRestSpread": true
		}
	},
	"rules": {
		"strict": 1,
		"no-const-assign": 1,
		"no-this-before-super": 1,
		"no-undef": 1,
		"no-unreachable": 1,
		"no-unused-vars": 1,
		"constructor-super": 1,
		"valid-typeof": 1,
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "double"],
		"semi": ["error", "always"],
		"indent": [1, "tab", { "SwitchCase": 1 }],
		"no-console": 0
	}
}
